{
  "name": "nick-site",
  "version": "1.0.0",
  "description": "My website",
  "main": "./src/entry.js",
  "scripts": {
    "back": "cross-env NODE_ENV=development nodemon --ignore ./webpack.config.js --ignore src/ --ignore dist/ --delay 2 ./server.js",
    "back-staging": "cross-env NODE_ENV=staging nodemon --ignore ./webpack.config.js --ignore src/ --ignore dist/ --delay 2 ./server.js",
    "build": "cross-env NODE_ENV=production webpack",
    "prettier-check": "prettier -c \"**/*.js\"",
    "dev": "concurrently npm:back npm:front",
    "staging": "concurrently npm:back-staging npm:front-staging",
    "dev-network": "concurrently npm:back npm:front-network",
    "eslint": "eslint ./src",
    "prettier-fix": "prettier --write \"**/*.js\"",
    "front": "cross-env NODE_ENV=development webpack-dev-server --inline --colors --history-api-fallback",
    "front-staging": "webpack-dev-server --inline --colors --history-api-fallback",
    "front-network": "cross-env NODE_ENV=development webpack-dev-server --inline --colors --history-api-fallback --host 0.0.0.0",
    "heroku-postbuild": "npm run knex-migrate-prod && npm run build",
    "knex-migrate-prod": "knex migrate:latest --env production",
    "knex-migrate-stage": "knex migrate:latest --env staging --verbose",
    "knex-migrate-stage-down": "knex migrate:down --env staging",
    "knex-migrate": "knex migrate:latest --env development --verbose",
    "knex-migrate+1": "knex migrate:up --env development",
    "knex-migrate-1": "knex migrate:down --env development",
    "knex-new-migration": "knex migrate:make name_goes_here",
    "db:up": "docker-compose up -d",
    "db:down": "docker-compose down",
    "db:reset": "docker-compose down && docker volume rm nick-site_postgres-data",
    "db:connect": "docker exec -it nick-site_sql-db_1 psql test_db",
    "server": "node ./server.js",
    "sim-deploy": "rm -rf node_modules && npm ci && npm run heroku-postbuild && npm run server",
    "sim-deploy-win": "npm ci && npm run heroku-postbuild && npm run server",
    "start": "npm run dev",
    "test": "npm run prettier-check",
    "update-pokemon-moves": "wget -O ./src/components/pokemon/showdown/moves.js https://raw.githubusercontent.com/smogon/pokemon-showdown/master/data/moves.js",
    "update-pokemon-pokedex": "wget -O ./src/components/pokemon/showdown/pokedex.js https://raw.githubusercontent.com/smogon/pokemon-showdown/master/data/pokedex.js",
    "update-pokemon": "npm run update-pokemon-pokedex && npm run update-pokemon-moves"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nick-ng/nick-site.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nick-ng/nick-site/issues"
  },
  "homepage": "https://github.com/nick-ng/nick-site",
  "dependencies": {
    "@babel/compat-data": "^7.11.0",
    "@babel/core": "^7.11.0",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-optional-chaining": "^7.11.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@babel/runtime": "^7.11.0",
    "@loadable/component": "^5.13.1",
    "axios": "^0.21.0",
    "babel-loader": "^8.1.0",
    "camelcase-keys": "^6.2.2",
    "classnames": "^2.2.6",
    "compression": "^1.6.2",
    "concurrently": "^5.2.0",
    "contentful": "^7.14.6",
    "cross-env": "^7.0.3",
    "css-loader": "^3.6.0",
    "cubejs": "^1.3.2",
    "dayjs": "^1.10.4",
    "dotenv": "^8.2.0",
    "express": "^4.14.1",
    "fsevents": "*",
    "html-webpack-plugin": "^5.3.1",
    "knex": "^0.95.2",
    "lodash": "^4.17.19",
    "md5": "^2.3.0",
    "moment": "^2.27.0",
    "papaparse": "^5.3.0",
    "path-browserify": "^1.0.1",
    "pg": "^8.5.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-markdown": "^4.3.1",
    "react-router-dom": "^5.2.0",
    "redis": "^3.0.2",
    "remark": "^13.0.0",
    "remark-gfm": "^1.0.0",
    "seedrandom": "^3.0.5",
    "style-loader": "^0.13.1",
    "styled-components": "^5.1.1",
    "uuid": "^7.0.3",
    "vis-timeline": "^7.3.7",
    "webpack": "^5.27.1",
    "webpack-cli": "^4.5.0",
    "ws": "^7.3.1"
  },
  "devDependencies": {
    "husky": "^4.2.5",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "webpack-dev-server": "^3.11.0"
  },
  "optionalDependencies": {
    "fsevents": "*"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
